let array=[2,3,4];
let input;
do{
   input=prompt("inter the array number")

    input=Number.parseInt(input)
    array.push(input)
    console.log(array)
}while(input!=0);
console.log(array)

    
let array2=[2,3,4]
array2.filter((a)=>
{
    return a%2==0;
})


array2.map((a)=>
{
    return a*a;
})

array2.reduce((a1,a2)=>
{
    return a1*a2;
})
